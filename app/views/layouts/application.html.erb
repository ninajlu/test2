<!DOCTYPE html>
<html>
<head>
  <title>Locate</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if alert %>
<div class="alert alert-error">
<%= alert %>
</div>
<% end %>
<% if notice %>
<div class="alert alert-success">
<%= notice %>
</div>
<% end %>
<%= yield %>
<% if user_signed_in? %>
<li><%= link_to 'Logout',
destroy_user_session_path,
method: :delete %></li>
<% end %>
</body>
</html>
